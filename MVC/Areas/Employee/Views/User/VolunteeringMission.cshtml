                            @if (Model[0].MissionRatings.Count == 0)
                            {
                                <img src="~/Images/star.png" alt="" />
                                <img src="~/Images/star.png" alt="" />
                                <img src="~/Images/star.png" alt="" />
                                <img src="~/Images/star.png" alt="" />
                                <img src="~/Images/star.png" alt="" />
                                <div class="mt-1">
                                    ( by 0 volunteers)
                                </div>
                            }
                            else
                            {
                                @foreach (var rate in Model[0].MissionRatings)
                                {
                                    sum = sum + rate.Rating;
                                    count++;
                                }
                                @for (var i = 0; i < (sum / count); i++)
                                {
                                    <img src="~/images/selected-star.png" alt="">
                                }
                                @for (var i = 0; i < 5 - (sum / count); i++)
                                {
                                    <img src="~/images/star.png" alt="">
                                }
                                <div class="mt-1">
                                    ( by @count volunteers)
                                </div>
                            }
                        </div>



